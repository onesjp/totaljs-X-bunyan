{
  "name": "totaljs-x-bunyan",
  "version": "1.0.0",
  "description": "",
  "main": "release.js",
  "dependencies": {
    "bunyan": "^1.8.12",
    "moment": "^2.29.2",
    "total.js": "^3.2.4"
  },
  "devDependencies": {
    "standard": "^14.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onesjp/totaljs-X-bunyan.git"
  },
